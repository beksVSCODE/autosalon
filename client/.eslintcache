se\\client\\src\\components\\TypeBar.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\modals\\CreateBrand.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\modals\\CreateType.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\modals\\CreateDevice.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\DeviceItem.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\http\\basketAPI.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\modals\\CreateCar.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\pages\\LandingPage.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\BookingForm.js",["169"],"import React, { useState, useContext } from 'react';\r\nimport { Form, Button, Row, Col, Alert } from 'react-bootstrap';\r\nimport DatePicker from 'react-datepicker';\r\nimport { ru } from 'date-fns/locale';\r\nimport \"react-datepicker/dist/react-datepicker.css\";\r\nimport PhoneInput from 'react-phone-input-2';\r\nimport 'react-phone-input-2/lib/style.css';\r\nimport { createBooking } from '../http/bookingAPI';\r\nimport { observer } from 'mobx-react-lite';\r\nimport { useHistory } from 'react-router-dom';\r\nimport { BOOKING_SUCCESS_ROUTE, LOGIN_ROUTE } from '../utils/consts';\r\nimport { Context } from '../index';\r\n\r\nconst BookingForm = observer(({ carId, onSuccess }) => {\r\n    const { user } = useContext(Context);\r\n    const history = useHistory();\r\n    const [loading, setLoading] = useState(false);\r\n    const [booking, setBooking] = useState({\r\n        full_name: '',\r\n        phone: '',\r\n        email: '',\r\n        type: 'test_drive',\r\n        date: new Date(),\r\n        comment: '',\r\n        carId: carId\r\n    });\r\n\r\n    const [error, setError] = useState('');\r\n\r\n    const handleSubmit = async (e) => {\r\n        e.preventDefault();\r\n        setError('');\r\n        setLoading(true);\r\n\r\n        if (!user.isAuth) {\r\n            history.push(LOGIN_ROUTE);\r\n            return;\r\n        }\r\n\r\n        if (!booking.full_name || !booking.email || !booking.phone || !booking.date) {\r\n            setError('Пожалуйста, заполните все обязательные поля');\r\n            setLoading(false);\r\n            return;\r\n        }\r\n\r\n        // Проверка email\r\n        const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\r\n        if (!emailRegex.test(booking.email)) {\r\n            setError('Пожалуйста, введите корректный email адрес');\r\n            setLoading(false);\r\n            return;\r\n        }\r\n\r\n        // Проверка телефона\r\n        const phoneNumber = booking.phone.startsWith('+') ? booking.phone : '+' + booking.phone;\r\n        if (!phoneNumber.match(/^\\+7\\d{10}$/)) {\r\n            setError('Пожалуйста, введите корректный номер телефона в формате +7XXXXXXXXXX');\r\n            setLoading(false);\r\n            return;\r\n        }\r\n\r\n        try {\r\n            const formattedBooking = {\r\n                ...booking,\r\n                phone: phoneNumber,\r\n                date: booking.date.toISOString()\r\n            };\r\n\r\n            const response = await createBooking(formattedBooking);\r\n\r\n            if (onSuccess) {\r\n                onSuccess();\r\n            }\r\n            history.push(BOOKING_SUCCESS_ROUTE);\r\n        } catch (err) {\r\n            console.error('Ошибка при отправке формы:', err);\r\n            if (err.response?.status === 401) {\r\n                setError('Необходимо авторизоваться для создания бронирования');\r\n                setTimeout(() => {\r\n                    history.push(LOGIN_ROUTE);\r\n                }, 2000);\r\n            } else {\r\n                setError(err.response?.data?.message || 'Произошла ошибка при создании бронирования. Пожалуйста, попробуйте позже.');\r\n            }\r\n        } finally {\r\n            setLoading(false);\r\n        }\r\n    };\r\n\r\n    const handleInputChange = (name, value) => {\r\n        setBooking(prev => ({\r\n            ...prev,\r\n            [name]: value\r\n        }));\r\n        setError('');\r\n    };\r\n\r\n    // Минимальная дата - завтра\r\n    const minDate = new Date();\r\n    minDate.setDate(minDate.getDate() + 1);\r\n\r\n    // Максимальная дата - через 14 дней\r\n    const maxDate = new Date();\r\n    maxDate.setDate(maxDate.getDate() + 14);\r\n\r\n    return (\r\n        <Form onSubmit={handleSubmit}>\r\n            {error && (\r\n                <Alert variant=\"danger\" className=\"mb-3\">\r\n                    {error}\r\n                </Alert>\r\n            )}\r\n\r\n            <Row>\r\n                <Col md={6}>\r\n                    <Form.Group className=\"mb-3\">\r\n                        <Form.Label>ФИО*</Form.Label>\r\n                        <Form.Control\r\n                            required\r\n                            type=\"text\"\r\n                            value={booking.full_name}\r\n                            onChange={(e) => handleInputChange('full_name', e.target.value)}\r\n                            placeholder=\"Введите ваше полное имя\"\r\n                            isInvalid={error && !booking.full_name}\r\n                        />\r\n                    </Form.Group>\r\n\r\n                    <Form.Group className=\"mb-3\">\r\n                        <Form.Label>Номер телефона*</Form.Label>\r\n                        <PhoneInput\r\n                            country={'ru'}\r\n                            value={booking.phone}\r\n                            onChange={(phone) => handleInputChange('phone', phone)}\r\n                            inputProps={{\r\n                                required: true\r\n                            }}\r\n                            isValid={booking.phone.length === 11}\r\n                        />\r\n                    </Form.Group>\r\n\r\n                    <Form.Group className=\"mb-3\">\r\n                        <Form.Label>Email*</Form.Label>\r\n                        <Form.Control\r\n                            required\r\n                            type=\"email\"\r\n                            value={booking.email}\r\n                            onChange={(e) => handleInputChange('email', e.target.value)}\r\n                            placeholder=\"example@domain.com\"\r\n                            isInvalid={error && !booking.email}\r\n                        />\r\n                    </Form.Group>\r\n                </Col>\r\n\r\n                <Col md={6}>\r\n                    <Form.Group className=\"mb-3\">\r\n                        <Form.Label>Дата тест-драйва*</Form.Label>\r\n                        <DatePicker\r\n                            selected={booking.date}\r\n                            onChange={(date) => handleInputChange('date', date)}\r\n                            showTimeSelect\r\n                            timeFormat=\"HH:mm\"\r\n                            timeIntervals={60}\r\n                            timeCaption=\"Время\"\r\n                            dateFormat=\"dd.MM.yyyy HH:mm\"\r\n                            minDate={minDate}\r\n                            maxDate={maxDate}\r\n                            minTime={new Date().setHours(9, 0)}\r\n                            maxTime={new Date().setHours(18, 0)}\r\n                            locale={ru}\r\n                            className={`form-control ${error && !booking.date ? 'is-invalid' : ''}`}\r\n                            required\r\n                        />\r\n                        <Form.Text className=\"text-muted\">\r\n                            Выберите удобные дату и время (с 9:00 до 18:00)\r\n                        </Form.Text>\r\n                    </Form.Group>\r\n\r\n                    <Form.Group className=\"mb-3\">\r\n                        <Form.Label>Комментарий</Form.Label>\r\n                        <Form.Control\r\n                            as=\"textarea\"\r\n                            rows={3}\r\n                            value={booking.comment}\r\n                            onChange={(e) => handleInputChange('comment', e.target.value)}\r\n                            placeholder=\"Дополнительная информация\"\r\n                        />\r\n                    </Form.Group>\r\n                </Col>\r\n            </Row>\r\n\r\n            <div className=\"d-grid gap-2\">\r\n                <Button\r\n                    variant=\"primary\"\r\n                    type=\"submit\"\r\n                    disabled={loading}\r\n                >\r\n                    {loading ? 'Отправка...' : 'Отправить заявку'}\r\n                </Button>\r\n            </div>\r\n            <Form.Text className=\"text-muted mt-2\">\r\n                * - обязательные поля\r\n            </Form.Text>\r\n        </Form>\r\n    );\r\n});\r\n\r\nexport default BookingForm;\r\n",["170","171"],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\http\\bookingAPI.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\pages\\BookingSuccessPage.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\pages\\Profile.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\AdminBookings.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\utils\\dateUtils.js",["172"],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\TimeSlotPicker.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\services\\BookingService.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\BookingList.js",[],"C:\\Users\\Lenovo\\Desktop\\online-store-full-course\\client\\src\\components\\modals\\CreateBooking.js",[],"C:\\